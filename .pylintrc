[MASTER]
load-plugins=pylint_django, pylint_celery
django-settings-module=config.test
ignore=migrations, tests

[FORMAT]
max-line-length=120
output-format=colorized

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,import-outside-toplevel,too-few-public-methods,cyclic-import,duplicate-code,modelform-uses-exclude,missing-timeout,line-too-long

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
